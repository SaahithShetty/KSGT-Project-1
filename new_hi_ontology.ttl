@prefix hi: <http://www.semanticweb.org/hi_ontology#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

hi:AIOutputTransparency a hi:AI_Concept ;
    hi:describedIn hi:HybridIntelligenceGenerativeDesign ;
    hi:hasDescription "The degree to which the output generated by an AI system is understandable and interpretable to the human user" ;
    hi:hasName "AI Output Transparency" .

hi:Absolutist a hi:AI_Concept ;
    hi:describedIn hi:ExosoulEthicalProfiling ;
    hi:hasDescription "Principled idealists who endorse both reliance on moral standards and striving to minimize harm" ;
    hi:hasName "Absolutist" ;
    hi:partOf hi:EthicsPositionTheory .

hi:ArchitecturalExpert a hi:Human_Agent ;
    hi:achievesScore hi:PartnershipScore ;
    hi:describedIn hi:HybridIntelligenceGenerativeDesign ;
    hi:evaluatedWith hi:HITAM_Evaluation ;
    hi:hasDescription "Human expert in architectural design who trains and personalizes the generative design assistant" ;
    hi:hasName "Architectural Expert" ;
    hi:interactsWith hi:GenerativeDesignAssistant ;
    hi:performsTask hi:FloorPlanDesignTask,
        hi:GenerativeDesignTrainingTask .

hi:BehaviorPredictionTask a hi:Task ;
    hi:describedIn hi:ExosoulEthicalProfiling ;
    hi:hasDescription "Predicting digital behaviors based on ethical profiles" ;
    hi:hasName "Behavior Prediction" ;
    hi:partOf hi:EthicalProfilingScenario .

hi:BehavioralTrustMeasurement a hi:AI_Concept ;
    hi:describedIn hi:TrustInterventionsHumanAI ;
    hi:hasDescription "Using behavioral indicators like intervention frequency to measure trust in human-AI interactions" ;
    hi:hasName "Behavioral Trust Measurement" ;
    hi:uses hi:InterventionFrequency .

hi:CaseBasedReasoningSystem a hi:AI_Agent ;
    hi:describedIn hi:LandmarksCaseBasedReasoning ;
    hi:hasDescription "AI system that provides explanations by analogy with training examples using case-based reasoning principles" ;
    hi:hasName "Case-Based Reasoning System" ;
    hi:performsTask hi:AIInterpretabilityTask ;
    hi:uses hi:LandmarkCases,
        hi:PrecedentialConstraint .

hi:CaseBasedReasoningXAI a hi:AI_Concept ;
    hi:describedIn hi:LandmarksCaseBasedReasoning ;
    hi:hasDescription "The idea of providing explanation of AI decisions through analogy with relevant training examples, based on case-based reasoning principles" ;
    hi:hasName "Case-Based Reasoning for Explainable AI" ;
    hi:relatedTo hi:Trust,
        hi:UserControl ;
    hi:uses hi:LandmarkCases,
        hi:PrecedentialConstraint .

hi:CaseResearcher a hi:Human_Agent ;
    hi:achievesScore hi:COMPASConsistencyScore ;
    hi:describedIn hi:LandmarksCaseBasedReasoning ;
    hi:evaluatedWith hi:ConsistencyEvaluation ;
    hi:hasDescription "Human researcher analyzing machine learning training data as legal cases through the lens of precedential constraint" ;
    hi:hasName "Case Researcher" ;
    hi:performsTask hi:CaseAnalysisTask .

hi:Compliance a hi:AI_Concept ;
    hi:describedIn hi:TrustInterventionsHumanAI ;
    hi:hasDescription "When the human accepts the agent's decisions, a behavioral demonstration of trust" ;
    hi:hasName "Compliance" ;
    hi:positivelyCorrelatedWith hi:Trust .

hi:Conscientiousness a hi:AI_Concept ;
    hi:describedIn hi:ExosoulEthicalProfiling ;
    hi:hasDescription "Personality trait reflecting persistence, orderliness, self-regulation and integrity" ;
    hi:hasName "Conscientiousness" .

hi:ContextualThompsonSampling a hi:AI_Concept ;
    hi:describedIn hi:HumanCenteredAIDementiaCare ;
    hi:hasDescription "Reinforcement learning algorithm that adapts to each meal's context to optimize interventions" ;
    hi:hasName "Contextual Thompson Sampling" ;
    hi:partOf hi:ReinforcementLearningDementiaCare .

hi:DiabetesUseCase a hi:Scenario ;
    hi:describedIn hi:ConversationalAgentDiaryConstruction ;
    hi:hasDescription "Supporting lifestyle changes for patients with Type 2 Diabetes Mellitus" ;
    hi:hasName "Diabetes Management" ;
    hi:relatedTo hi:DiaryConstructionScenario .

hi:DiaryUser a hi:Human_Agent ;
    hi:describedIn hi:ConversationalAgentDiaryConstruction ;
    hi:hasDescription "Person interacting with the conversational agent to build a diary" ;
    hi:hasName "Diary User/Patient" ;
    hi:interactsWith hi:HybridIntelligenceAgent .

hi:DimensionOrdering a hi:AI_Concept ;
    hi:describedIn hi:LandmarksCaseBasedReasoning ;
    hi:hasDescription "A method to determine the orders for dimensions in case-based reasoning by using statistical trends like logistic regression coefficients" ;
    hi:hasName "Dimension Ordering" ;
    hi:partOf hi:PrecedentialConstraint .

hi:DynamicTrust a hi:AI_Concept ;
    hi:describedIn hi:TrustInterventionsHumanAI ;
    hi:hasDescription "Trust as a dynamic process that changes over time, not a static one-time measurement" ;
    hi:hasName "Dynamic Trust" ;
    hi:relatedTo hi:Trust .

hi:EnvironmentUncertainty a hi:AI_Concept ;
    hi:describedIn hi:TrustInterventionsHumanAI ;
    hi:hasDescription "Non-determinism in the environment, where external events occur outside the control of the agent and human" ;
    hi:hasName "Environment Uncertainty" ;
    hi:influences hi:InterventionFrequency,
        hi:MiscalibratedTrust ;
    hi:negativelyCorrelatedWith hi:Trust .

hi:EthicalProfilingTask a hi:Task ;
    hi:describedIn hi:ExosoulEthicalProfiling ;
    hi:hasDescription "Creating ethical profiles to predict digital behaviors" ;
    hi:hasName "Ethical Profiling" ;
    hi:partOf hi:EthicalProfilingScenario .

hi:Exceptionist a hi:AI_Concept ;
    hi:describedIn hi:ExosoulEthicalProfiling ;
    hi:hasDescription "Conventionalists who tolerate exceptions to moral standards when benefits offset potential harmful consequences" ;
    hi:hasName "Exceptionist" ;
    hi:partOf hi:EthicsPositionTheory .

hi:ForcingRelation a hi:AI_Concept ;
    hi:describedIn hi:LandmarksCaseBasedReasoning ;
    hi:hasDescription "Relation that models a fortiori reasoning: if a fact situation F has been decided for outcome s and we encounter a situation G that is at least as good for s, then G should also be decided for s" ;
    hi:hasName "Forcing Relation" ;
    hi:partOf hi:PrecedentialConstraint .

hi:GoalMisalignment a hi:AI_Concept ;
    hi:describedIn hi:TrustInterventionsHumanAI ;
    hi:hasDescription "When an agent's behavior or decisions do not fully align with what the user wants to accomplish" ;
    hi:hasName "Goal Misalignment" ;
    hi:influences hi:InterventionFrequency ;
    hi:negativelyCorrelatedWith hi:Trust ;
    hi:oppositeOf hi:GoalAlignment .

hi:HI_TAM_Concept a hi:AI_Concept ;
    hi:describedIn hi:HybridIntelligenceGenerativeDesign ;
    hi:hasDescription "A model adapted from AI-TAM incorporating key aspects from HI including AI transparency and user control to support both virtual assistant training and general human-AI mutual learning" ;
    hi:hasName "Hybrid Intelligence Technology Acceptance Model" .

hi:HumanSimulator a hi:AI_Concept ;
    hi:describedIn hi:HumanCenteredAIDementiaCare ;
    hi:hasDescription "Simulation of behaviors and responses of users with dementia to test AI system effectiveness in a controlled environment" ;
    hi:hasName "Human Simulator" .

hi:HybridIntelligenceNarrative a hi:AI_Concept ;
    hi:describedIn hi:HybridIntelligenceGenerativeDesign ;
    hi:hasDescription "A narrative designed to create a psychologically safe space for co-creation without the fear of job-replacement" ;
    hi:hasName "Hybrid Intelligence Narrative" .

hi:LinearUpperConfidenceBound a hi:AI_Concept ;
    hi:describedIn hi:HumanCenteredAIDementiaCare ;
    hi:hasDescription "Contextual reinforcement learning algorithm for decision-making under uncertainty" ;
    hi:hasName "Linear Upper Confidence Bound" ;
    hi:partOf hi:ReinforcementLearningDementiaCare .

hi:Machiavellianism a hi:AI_Concept ;
    hi:describedIn hi:ExosoulEthicalProfiling ;
    hi:hasDescription "Personality trait reflecting manipulative tactics, cynicism, and 'the ends justify the means' attitude" ;
    hi:hasName "Machiavellianism" ;
    hi:negativelyCorrelatedWith hi:Idealism ;
    hi:positivelyCorrelatedWith hi:Relativism .

hi:Narcissism a hi:AI_Concept ;
    hi:describedIn hi:ExosoulEthicalProfiling ;
    hi:hasDescription "Personality trait reflecting inflated opinion of self, entitlement, superiority and need for admiration" ;
    hi:hasName "Narcissism" .

hi:NavigatorHuman a hi:Human_Agent ;
    hi:describedIn hi:TrustInterventionsHumanAI ;
    hi:hasDescription "Human using the AI navigation system and making intervention decisions" ;
    hi:hasName "Navigator Human" ;
    hi:interactsWith hi:NavigationAI ;
    hi:performsTask hi:Intervention,
        hi:NavigationTask .

hi:PatientRecoveryUseCase a hi:Scenario ;
    hi:describedIn hi:ConversationalAgentDiaryConstruction ;
    hi:hasDescription "Monitoring recovery and functioning of elderly patients" ;
    hi:hasName "Patient Recovery and Functioning Patterns" ;
    hi:relatedTo hi:DiaryConstructionScenario .

hi:PwDUser a hi:Human_Agent ;
    hi:describedIn hi:HumanCenteredAIDementiaCare ;
    hi:hasDescription "User with early-stage dementia who needs assistance with eating and drinking routines" ;
    hi:hasName "Person with Early-Stage Dementia" ;
    hi:interactsWith hi:WhoTakesCareAI .

hi:Situationist a hi:AI_Concept ;
    hi:describedIn hi:ExosoulEthicalProfiling ;
    hi:hasDescription "Idealistic contextualists who value minimizing harm rather than reliance on moral standards" ;
    hi:hasName "Situationist" ;
    hi:partOf hi:EthicsPositionTheory .

hi:SoftwareExoskeleton a hi:AI_Concept ;
    hi:describedIn hi:ExosoulEthicalProfiling ;
    hi:hasDescription "Software shield that protects users and mediates actions according to moral preferences" ;
    hi:hasName "Software Exoskeleton" ;
    hi:partOf hi:ExosoulProject ;
    hi:uses hi:LegalistProfile,
        hi:OpportunistProfile,
        hi:SensibleProfile,
        hi:VirtuousProfile .

hi:Subjectivist a hi:AI_Concept ;
    hi:describedIn hi:ExosoulEthicalProfiling ;
    hi:hasDescription "Realists who do not endorse moral standards or the avoidance of harmful consequences" ;
    hi:hasName "Subjectivist" ;
    hi:partOf hi:EthicsPositionTheory .

hi:ThreeStageEscalationScenario a hi:AI_Concept ;
    hi:describedIn hi:HumanCenteredAIDementiaCare ;
    hi:hasDescription "Approach with increasing intensity of signals to gently guide dementia patients through eating routines" ;
    hi:hasName "Three-Stage Escalated Eating Scenario" .

hi:ToxicityUseCase a hi:Scenario ;
    hi:describedIn hi:ConversationalAgentDiaryConstruction ;
    hi:hasDescription "Monitoring side effects of cancer treatments like chemotherapy" ;
    hi:hasName "Cancer Therapy Toxicity Monitoring" ;
    hi:relatedTo hi:DiaryConstructionScenario .

hi:achievesScore a owl:ObjectProperty ;
    rdfs:label "achievesScore" ;
    rdfs:domain hi:Agent ;
    rdfs:range hi:Score .

hi:appliesTo a owl:ObjectProperty ;
    rdfs:label "appliesTo" ;
    rdfs:domain hi:Evaluation ;
    rdfs:range hi:Task .

hi:basedOn a owl:ObjectProperty ;
    rdfs:label "basedOn" ;
    rdfs:domain hi:Scenario ;
    rdfs:range hi:ResearchPaper .

hi:describedIn a owl:ObjectProperty ;
    rdfs:label "describedIn" ;
    rdfs:range hi:ResearchPaper .

hi:evaluatedWith a owl:ObjectProperty ;
    rdfs:label "evaluatedWith" ;
    rdfs:domain hi:Agent ;
    rdfs:range hi:Evaluation .

hi:hasAuthor a owl:DatatypeProperty ;
    rdfs:label "hasAuthor" ;
    rdfs:domain hi:ResearchPaper ;
    rdfs:range xsd:string .

hi:hasDOI a owl:DatatypeProperty ;
    rdfs:label "hasDOI" ;
    rdfs:domain hi:ResearchPaper ;
    rdfs:range xsd:string .

hi:hasDescription a owl:DatatypeProperty ;
    rdfs:label "hasDescription" ;
    rdfs:range xsd:string .

hi:hasName a owl:DatatypeProperty ;
    rdfs:label "hasName" ;
    rdfs:range xsd:string .

hi:hasResult a owl:ObjectProperty ;
    rdfs:label "hasResult" ;
    rdfs:domain hi:Evaluation ;
    rdfs:range hi:Score .

hi:hasTitle a owl:DatatypeProperty ;
    rdfs:label "hasTitle" ;
    rdfs:domain hi:ResearchPaper ;
    rdfs:range xsd:string .

hi:hasValue a owl:DatatypeProperty ;
    rdfs:label "hasValue" ;
    rdfs:domain hi:Score ;
    rdfs:range xsd:float .

hi:hasYear a owl:DatatypeProperty ;
    rdfs:label "hasYear" ;
    rdfs:domain hi:ResearchPaper ;
    rdfs:range xsd:integer .

hi:influences a owl:ObjectProperty ;
    rdfs:label "influences" .

hi:interactsWith a owl:ObjectProperty ;
    rdfs:label "interactsWith" ;
    rdfs:domain hi:Human_Agent ;
    rdfs:range hi:AI_Agent .

hi:negativelyCorrelatedWith a owl:ObjectProperty ;
    rdfs:label "negativelyCorrelatedWith" .

hi:partOf a owl:ObjectProperty ;
    rdfs:label "partOf" ;
    rdfs:domain hi:Task ;
    rdfs:range hi:Scenario .

hi:performsTask a owl:ObjectProperty ;
    rdfs:label "performsTask" ;
    rdfs:domain hi:Agent ;
    rdfs:range hi:Task .

hi:positivelyCorrelatedWith a owl:ObjectProperty ;
    rdfs:label "positivelyCorrelatedWith" .

hi:prevents a owl:ObjectProperty ;
    rdfs:label "prevents" .

hi:uses a owl:ObjectProperty ;
    rdfs:label "uses" .

hi:AIInterpretabilityTask a hi:Task ;
    hi:describedIn hi:LandmarksCaseBasedReasoning ;
    hi:hasDescription "The task of elucidating the decision making process of machine learning systems" ;
    hi:hasName "AI Interpretability" ;
    hi:partOf hi:CaseBasedReasoningScenario .

hi:AgentCapability a hi:AI_Concept ;
    hi:describedIn hi:TrustInterventionsHumanAI ;
    hi:hasDescription "The agent's ability to act as a decision aid, reflecting partial alignment between what the agent optimizes for and the user's goals" ;
    hi:hasName "Agent Capability" ;
    hi:influences hi:InterventionFrequency,
        hi:Trust .

hi:ConsistencyEvaluation a hi:Evaluation ;
    hi:appliesTo hi:CaseAnalysisTask ;
    hi:describedIn hi:LandmarksCaseBasedReasoning ;
    hi:hasDescription "Evaluation of the degree to which training data obeys the precedent set by other examples" ;
    hi:hasName "Consistency Evaluation" ;
    hi:hasResult hi:COMPASConsistencyScore .

hi:ContextualMultiArmedBandit a hi:AI_Concept ;
    hi:describedIn hi:HumanCenteredAIDementiaCare ;
    hi:hasDescription "Formulation to address unknown preferences of people with dementia in mealtime interventions" ;
    hi:hasName "Contextual Multi-Armed Bandit" ;
    hi:partOf hi:ReinforcementLearningDementiaCare .

hi:ContinualLearningLoops a hi:AI_Concept ;
    hi:describedIn hi:HybridIntelligenceGenerativeDesign ;
    hi:hasDescription "Human-centered design principles to seamlessly integrate AI training into the expert's task workflow" ;
    hi:hasName "Continual Learning Loops" .

hi:DementiaCareSystemEvaluation a hi:Evaluation ;
    hi:appliesTo hi:EatingReminderTask ;
    hi:describedIn hi:HumanCenteredAIDementiaCare ;
    hi:hasDescription "Evaluation of the AI system's effectiveness in adapting to diverse user preferences and behaviors" ;
    hi:hasName "Dementia Care System Evaluation" ;
    hi:hasResult hi:SimulationScore .

hi:DiaryConstructionTask a hi:Task ;
    hi:describedIn hi:ConversationalAgentDiaryConstruction ;
    hi:hasDescription "Creating a structured diary through conversation" ;
    hi:hasName "Diary Construction" ;
    hi:partOf hi:DiaryConstructionScenario .

hi:EmpathicAI a hi:AI_Concept ;
    hi:describedIn hi:HumanCenteredAIDementiaCare ;
    hi:hasDescription "AI designed to be perceived as empathic by users with cognitive impairments, improving acceptability and usability" ;
    hi:hasName "Empathic AI" .

hi:EpisodicKnowledgeGraph a hi:AI_Concept ;
    hi:describedIn hi:ConversationalAgentDiaryConstruction ;
    hi:hasDescription "A timeline of events represented as a knowledge graph" ;
    hi:hasName "Episodic Knowledge Graph" .

hi:ExosoulProject a hi:AI_Concept ;
    hi:describedIn hi:ExosoulEthicalProfiling ;
    hi:hasDescription "Project aiming to develop a personalized software exoskeleton to protect and support citizen's ethics and privacy" ;
    hi:hasName "Exosoul Project" .

hi:GRaSPFramework a hi:AI_Concept ;
    hi:describedIn hi:ConversationalAgentDiaryConstruction ;
    hi:hasDescription "Framework to represent mentions of events in conversation" ;
    hi:hasName "GRaSP Framework" .

hi:GenerativeDesignAssistant a hi:AI_Agent ;
    hi:describedIn hi:HybridIntelligenceGenerativeDesign ;
    hi:hasDescription "AI system trained to assist human experts by generating design solutions based on their inputs and preferences" ;
    hi:hasName "Generative Design Assistant" ;
    hi:performsTask hi:FloorPlanDesignTask ;
    hi:uses hi:ContinualLearningLoops,
        hi:ProgrammableCommonLanguage .

hi:GoalAlignment a hi:AI_Concept ;
    hi:describedIn hi:TrustInterventionsHumanAI ;
    hi:hasDescription "The degree to which the human's goal matches the AI's programmed goal" ;
    hi:hasName "Goal Alignment" .

hi:HITAM_Evaluation a hi:Evaluation ;
    hi:appliesTo hi:GenerativeDesignTrainingTask ;
    hi:describedIn hi:HybridIntelligenceGenerativeDesign ;
    hi:hasDescription "Evaluation of human experts' willingness to build partnership with generative design assistants" ;
    hi:hasName "HI-TAM Evaluation" ;
    hi:hasResult hi:PartnershipScore .

hi:Human a owl:Class ;
    rdfs:label "Human" ;
    rdfs:comment "General class representing human beings" .

hi:HybridIntelligenceAgent a hi:AI_Agent ;
    hi:describedIn hi:ConversationalAgentDiaryConstruction ;
    hi:hasDescription "An agent that constructs a personal diary through conversation" ;
    hi:hasName "Hybrid Intelligence Conversational Agent" ;
    hi:performsTask hi:DiaryConstructionTask,
        hi:TimelineReconstructionTask,
        hi:WellBeingMonitoringTask ;
    hi:uses hi:EpisodicKnowledgeGraph,
        hi:GRaSPFramework,
        hi:PerspectiveDrivenIntent,
        hi:PropertyDrivenIntent,
        hi:SimpleEventModel,
        hi:TimelineDrivenIntent .

hi:Idealism a hi:AI_Concept ;
    hi:describedIn hi:ExosoulEthicalProfiling ;
    hi:hasDescription "Reflects absolute moral principles, oriented to truth, benevolence and avoiding harming others" ;
    hi:hasName "Idealism" ;
    hi:partOf hi:EthicsPositionTheory ;
    hi:positivelyCorrelatedWith hi:HonestyHumility .

hi:JITAI_Concept a hi:AI_Concept ;
    hi:describedIn hi:HumanCenteredAIDementiaCare ;
    hi:hasDescription "Design concept aiming at adapting to an individual's changing internal and contextual state to provide the right type and amount of support" ;
    hi:hasName "Just-in-Time Adaptive Intervention" .

hi:LegalistProfile a hi:AI_Concept ;
    hi:describedIn hi:ExosoulEthicalProfiling ;
    hi:hasDescription "Ethical profile with moderate idealism and relativism, and high normativism" ;
    hi:hasName "Legalist Profile" .

hi:MiscalibratedTrust a hi:AI_Concept ;
    hi:describedIn hi:TrustInterventionsHumanAI ;
    hi:hasDescription "When trust level doesn't match the actual capabilities of the system, leading to misuse or disuse" ;
    hi:hasName "Miscalibrated Trust" ;
    hi:subClassOf hi:Trust .

hi:NavigationAI a hi:AI_Agent ;
    hi:describedIn hi:TrustInterventionsHumanAI ;
    hi:hasDescription "AI agent that optimizes for either distance or time in a route planning task" ;
    hi:hasName "Navigation AI" ;
    hi:performsTask hi:NavigationTask ;
    hi:uses hi:AgentCapability .

hi:OpportunistProfile a hi:AI_Concept ;
    hi:describedIn hi:ExosoulEthicalProfiling ;
    hi:hasDescription "Ethical profile with low idealism, honesty/humility, conscientiousness and high relativism, Machiavellianism and narcissism" ;
    hi:hasName "Opportunist Profile" ;
    hi:influences hi:CopyrightInfringement,
        hi:PrivacyViolation ;
    hi:prevents hi:Caution .

hi:PerceivedPartnership a hi:AI_Concept ;
    hi:describedIn hi:HybridIntelligenceGenerativeDesign ;
    hi:hasDescription "User's subjective perception of the degree to which an AI is perceived as a collaborative partner in the interaction" ;
    hi:hasName "Perceived Partnership" .

hi:PerspectiveDrivenIntent a hi:AI_Concept ;
    hi:describedIn hi:ConversationalAgentDiaryConstruction ;
    hi:hasDescription "Low-level intents driven by possible and likely perspectives" ;
    hi:hasName "Perspective-Driven Intent" .

hi:ProgrammableCommonLanguage a hi:AI_Concept ;
    hi:describedIn hi:HybridIntelligenceGenerativeDesign ;
    hi:hasDescription "A grammar-based method for constructing common language between humans and algorithms allowing for the explicitation of individual experts' design goals" ;
    hi:hasName "Programmable Common Language" .

hi:PropertyDrivenIntent a hi:AI_Concept ;
    hi:describedIn hi:ConversationalAgentDiaryConstruction ;
    hi:hasDescription "Mid-level intents driven by necessary/possible/probable properties in the ontology" ;
    hi:hasName "Property-Driven Intent" .

hi:RLOptimizationTask a hi:Task ;
    hi:describedIn hi:HumanCenteredAIDementiaCare ;
    hi:hasDescription "Optimizing signal combinations based on user behavior and preferences using reinforcement learning" ;
    hi:hasName "RL Signal Optimization" ;
    hi:partOf hi:DementiaCareScenario .

hi:Relativism a hi:AI_Concept ;
    hi:describedIn hi:ExosoulEthicalProfiling ;
    hi:hasDescription "Relies on evaluations of situations, contexts and consequences, reflecting that harm may be necessary for good" ;
    hi:hasName "Relativism" ;
    hi:negativelyCorrelatedWith hi:HonestyHumility ;
    hi:partOf hi:EthicsPositionTheory .

hi:SensibleProfile a hi:AI_Concept ;
    hi:describedIn hi:ExosoulEthicalProfiling ;
    hi:hasDescription "Ethical profile with moderate idealism, low relativism, and high honesty/humility" ;
    hi:hasName "Sensible Profile" .

hi:SignalCombinations a hi:AI_Concept ;
    hi:describedIn hi:HumanCenteredAIDementiaCare ;
    hi:hasDescription "Combinations of low to high intensity signals including scent, music, light, image, voice, and video to nudge users" ;
    hi:hasName "Multimodal Signal Combinations" .

hi:SimpleEventModel a hi:AI_Concept ;
    hi:describedIn hi:ConversationalAgentDiaryConstruction ;
    hi:hasDescription "Formal model for defining events in someone's life" ;
    hi:hasName "Simple Event Model" .

hi:TimelineDrivenIntent a hi:AI_Concept ;
    hi:describedIn hi:ConversationalAgentDiaryConstruction ;
    hi:hasDescription "High-level intents driven by the timeline, e.g., verifying expected events" ;
    hi:hasName "Timeline-Driven Intent" .

hi:TimelineReconstructionTask a hi:Task ;
    hi:describedIn hi:ConversationalAgentDiaryConstruction ;
    hi:hasDescription "Reconstructing a timeline of events through conversation" ;
    hi:hasName "Timeline Reconstruction" ;
    hi:partOf hi:DiaryConstructionScenario .

hi:TrustInterventionsScenario a hi:Scenario ;
    hi:basedOn hi:TrustInterventionsHumanAI ;
    hi:describedIn hi:TrustInterventionsHumanAI ;
    hi:hasDescription "A scenario exploring interventions as trust indicators in human-AI collaborative tasks with misaligned goals" ;
    hi:hasName "Trust Interventions in Human-AI Collaboration" .

hi:UserControl a hi:AI_Concept ;
    hi:describedIn hi:HybridIntelligenceGenerativeDesign ;
    hi:hasDescription "The level of control and autonomy the user feels they have over the AI tool" ;
    hi:hasName "User Control" ;
    hi:positivelyCorrelatedWith hi:PerceivedPartnership .

hi:VirtuousProfile a hi:AI_Concept ;
    hi:describedIn hi:ExosoulEthicalProfiling ;
    hi:hasDescription "Ethical profile with high idealism, honesty/humility, conscientiousness and low relativism, Machiavellianism and narcissism" ;
    hi:hasName "Virtuous Profile" ;
    hi:influences hi:Caution ;
    hi:prevents hi:CopyrightInfringement,
        hi:PrivacyViolation .

hi:WellBeingMonitoringTask a hi:Task ;
    hi:describedIn hi:ConversationalAgentDiaryConstruction ;
    hi:hasDescription "Monitoring physical, social and mental functioning through conversation" ;
    hi:hasName "Well-Being Monitoring" ;
    hi:partOf hi:DiaryConstructionScenario .

hi:WhoTakesCareAI a hi:AI_Agent ;
    hi:achievesScore hi:SimulationScore ;
    hi:describedIn hi:HumanCenteredAIDementiaCare ;
    hi:evaluatedWith hi:DementiaCareSystemEvaluation ;
    hi:hasDescription "Personalized AI system that adapts to individual behaviors and provides suitable signals to nudge eating behavior" ;
    hi:hasName "Who Takes Care AI System" ;
    hi:performsTask hi:EatingReminderTask,
        hi:RLOptimizationTask ;
    hi:uses hi:ContextualMultiArmedBandit,
        hi:EmpathicAI,
        hi:JITAI_Concept,
        hi:ReinforcementLearningDementiaCare,
        hi:SignalCombinations .

hi:COMPASConsistencyScore a hi:Score ;
    hi:hasName "COMPAS Consistency Score" ;
    hi:hasValue "0.08"^^xsd:float .

hi:CaseAnalysisTask a hi:Task ;
    hi:describedIn hi:LandmarksCaseBasedReasoning ;
    hi:hasDescription "The task of analyzing case data to identify landmark cases and determine consistency with precedence" ;
    hi:hasName "Case Analysis" ;
    hi:partOf hi:CaseBasedReasoningScenario .

hi:CaseBasedReasoningScenario a hi:Scenario ;
    hi:basedOn hi:LandmarksCaseBasedReasoning ;
    hi:describedIn hi:LandmarksCaseBasedReasoning ;
    hi:hasDescription "A scenario for analyzing how machine learning systems draw on training data using theory of precedential constraint" ;
    hi:hasName "Case-Based Reasoning Scenario" .

hi:Caution a hi:AI_Concept ;
    hi:describedIn hi:ExosoulEthicalProfiling ;
    hi:hasDescription "Reading privacy policies and being cautious with personal information online" ;
    hi:hasName "Caution" .

hi:CopyrightInfringement a hi:AI_Concept ;
    hi:describedIn hi:ExosoulEthicalProfiling ;
    hi:hasDescription "Using software or content without owning a license" ;
    hi:hasName "Copyright Infringement" .

hi:DementiaCareScenario a hi:Scenario ;
    hi:basedOn hi:HumanCenteredAIDementiaCare ;
    hi:describedIn hi:HumanCenteredAIDementiaCare ;
    hi:hasDescription "A scenario for providing personalized interventions to support people with early-stage dementia in managing their eating and drinking routines" ;
    hi:hasName "Dementia Care Eating and Drinking Scenario" .

hi:EatingReminderTask a hi:Task ;
    hi:describedIn hi:HumanCenteredAIDementiaCare ;
    hi:hasDescription "Task of reminding people with dementia to eat and drink at appropriate times" ;
    hi:hasName "Eating Reminder" ;
    hi:partOf hi:DementiaCareScenario .

hi:EthicalProfilingScenario a hi:Scenario ;
    hi:basedOn hi:ExosoulEthicalProfiling ;
    hi:describedIn hi:ExosoulEthicalProfiling ;
    hi:hasDescription "A scenario for creating ethical profiles of users to predict digital behaviors" ;
    hi:hasName "Ethical Profiling Scenario" .

